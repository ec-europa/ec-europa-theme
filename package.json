{
  "name": "europa",
  "version": "1.0.0",
  "dependencies": {
    "grunt-cli": "~1.2.0"
  },
  "devDependencies": {
    "gh-pages": "^0.12.0",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "~1.0.0",
    "grunt-contrib-copy": "~1.0.0",
    "grunt-contrib-watch": "~1.0.0",
    "grunt-sass": "~2.0.0",
    "grunt-shell": "~2.1.0",
    "http-server": "^0.9.0",
    "kss": "~3.0.0-beta.16",
    "node-sass": "^4.1.1",
    "npm-run-all": "^4.0.2"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "clean": "grunt clean",
    "develop": "run-s clean dist watch",
    "dist": "grunt styleguide",
    "deploy": "gh-pages -d styleguide/assets",
    "server": "http-server ./styleguide/assets",
    "start": "run-p develop server",
    "postinstall": "find node_modules/ -name '*.info' -type f | xargs /bin/rm -f",
    "watch": "grunt"
  }
}
